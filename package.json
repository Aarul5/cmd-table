  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cmd-table": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "docs:dev": "vitepress dev docs",
