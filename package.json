    "csv",
    "json",
    "markdown",
    "tree",
    "nested",
    "merge",
    "ansi",
    "pretty-print"
  ],
  "author": {
    "name": "Arul",
    "url": "https://github.com/Aarul5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aarul5/cmd-table.git"
  },
  "bugs": {
    "url": "https://github.com/Aarul5/cmd-table/issues"
  },
  "homepage": "https://github.com/Aarul5/cmd-table#readme",
