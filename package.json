    "docs:preview": "vitepress preview docs",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "table",
    "terminal",
    "console",
    "ascii",
    "unicode",
    "responsive",
    "layout",
    "stream",
